		"useTabs": true,
		"tabWidth": 4,
		"endOfLine": "auto"
	},
	"devDependencies": {
		"@eslint/js": "^9.28.0",
		"@tailwindcss/typography": "^0.5.16",
		"concurrently": "^9.1.2",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.1"
	}
