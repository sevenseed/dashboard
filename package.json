		"typecheck:watch": "tsc --noEmit --watch --preserveWatchOutput --project tsconfig.watch.json"
	},
	"dependencies": {
		"@formspree/react": "^3.0.0",
		"@headlessui/react": "^2.2.4",
		"@heroicons/react": "^2.2.0",
		"@lostisworld/tailwind-mask": "^1.0.1",
		"@tailwindcss/postcss": "^4.1.8",
		"@types/cookie": "^1.0.0",
		"@types/node": "22.15.29",
		"@types/react": "19.1.6",
		"@types/react-dom": "19.1.5",
		"autoprefixer": "10.4.21",
		"change-case": "^5.4.4",
		"clsx": "^2.1.1",
		"eslint-config-next": "15.3.3",
		"headlessui": "^0.0.0",
		"heroicons": "^2.2.0",
		"just-compare": "^2.3.0",
		"just-omit": "^2.2.0",
		"just-pick": "^4.2.0",
		"next": "^15.3.3",
		"next-intl": "^4.1.0",
		"postcss": "^8.5.4",
		"prettier": "^3.5.3",
		"react": "19.1.0",
		"react-dom": "19.1.0",
