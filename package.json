		"printWidth": 88,
		"useTabs": true,
		"tabWidth": 4,
		"endOfLine": "auto"
	},
	"devDependencies": {
		"@eslint/js": "^9.4.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint__js": "^8.42.3",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
