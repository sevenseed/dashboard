		"@stripe/stripe-js": "^3.1.0",
		"@supabase/ssr": "^0.0.10",
		"@supabase/supabase-js": "^2.38.4",
		"@types/node": "20.5.1",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"autoprefixer": "10.4.15",
		"change-case": "^5.4.4",
		"clsx": "^2.1.0",
		"eslint-config-next": "13.4.19",
		"headlessui": "^0.0.0",
		"heroicons": "^2.0.18",
